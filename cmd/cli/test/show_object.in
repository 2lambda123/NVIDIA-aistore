echo "0123456789" > $OBJECT_1.txt
ais create bucket $BUCKET_1
ais put $OBJECT_1.txt $BUCKET_1/object_1.txt

ais show object $BUCKET_1/object_2.txt // FAIL "No such object "object_2.txt" in bucket "ais://$BUCKET_1""
ais show object ais://$BUCKET_2/object_1.txt // FAIL "Bucket "ais://$BUCKET_2" does not exist"

ais show object $BUCKET_1/object_1.txt
ais show object $BUCKET_1/object_1.txt --props=size,version
ais show object $BUCKET_1/object_1.txt --props=all

ais show object aiss://$BUCKET_1/object_1.txt // FAIL "invalid backend provider "aiss""
